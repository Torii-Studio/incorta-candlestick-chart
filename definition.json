{
  "icon": "./assets/candlestick-icon.png",
  "name": "candlestick-chart",
  "hint": "Display a candlestick chart",
  "options": {
    "aggregation": true,
    "individualFilter": true,
    "aggregateFilter": true
  },
  "settings": [
    {
      "name": {
        "key": "Configuration"
      },
      "settings": [
        {
          "name": {
            "key": "Scale Y-Axis"
          },
          "key": "scale",
          "defaultValue": true,
          "type": "boolean"
        }
      ]
    }
  ],
  "bindingsTrays": [
    {
      "key": "timestamp",
      "name": "Timestamp",
      "queryRole": "row",
