      "maxCount": 1
    },
    {
      "key": "close",
      "name": "Close",
      "queryRole": "measure",
      "minCount": 1,
      "maxCount": 1
    },
    {
      "key": "open",
      "name": "Open",
      "queryRole": "measure",
      "minCount": 1,
      "maxCount": 1
    },
    {
      "key": "high",
      "name": "High",
      "queryRole": "measure",
      "minCount": 1,
      "maxCount": 1
    },
    {
      "key": "low",
      "name": "Low",
      "queryRole": "measure",
      "minCount": 1,
      "maxCount": 1
    }
  ]
